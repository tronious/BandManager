{
  "globalHeaders": {
    "Content-Security-Policy": "frame-src 'self' https://www.youtube.com https://youtube.com; frame-ancestors 'self';"
  },
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/sw.js",
      "headers": {
        "Cache-Control": "no-store"
      }
    },
    {
      "route": "/service-worker.js",
      "headers": {
        "Cache-Control": "no-store"
      }
    },
    {
      "route": "/*",
      "headers": {
        "Cache-Control": "no-store",
        "Pragma": "no-cache",
        "Expires": "0"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.{css,js,json,ico,png,jpg,jpeg,svg,gif,webp,woff,woff2}"]
  }
}
